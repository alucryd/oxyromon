{
  "name": "oxyromon",
  "version": "0.18.0-beta1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check",
    "check:watch": "svelte-check --watch",
    "lint": "healthier && prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@popperjs/core": "^2.11.8",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "bootstrap": "^5.3.2",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "healthier": "^7.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "pretty-bytes": "^6.1.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-fa": "^3.0.4",
    "svelte-preprocess": "^5.1.3",
    "sveltestrap": "^5.11.3",
    "vite": "^5.0.10"
  },
  "type": "module",
  "healthier": {
    "plugins": [
      "svelte3"
    ],
    "ignorePatterns": [
      "build/",
      "target/"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false
  }
}
